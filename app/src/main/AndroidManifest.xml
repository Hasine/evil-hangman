        </activity>
        <activity
            android:name=".settings"
            android:label="@string/HEAD"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".HistoryViewActivity"
            android:label="@string/HEAD"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
